<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippet Format="1.0.0">
    <Header>
        <Title>Test Teardown Method</Title>
        <Shortcut>testteardown</Shortcut>
        <Description>Generate Test Teardown method</Description>
        <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
        </SnippetTypes>
    </Header>
    <Snippet>
        <Declarations>
            <Literal>
                <ID>methodName</ID>
                <ToolTip>The name of the Teardown method</ToolTip>
                <Default>TearDown</Default>
            </Literal>
        </Declarations>
        <Code Language="csharp" Format="CData"><![CDATA[
	[TearDown]
    public void $methodName$()
	{
        $end$
	}
	]]>
	</Code>
    </Snippet>
</CodeSnippet>