<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.1.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jQuery: Set data value on element(s)</Title>
      <Author>John Sheehan (@johnsheehan)</Author>
      <Shortcut>jqdataset</Shortcut>
      <HelpUrl>http://jquerysnippets.codeplex.com</HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>selector</ID>
          <ToolTip>Elements to set data on</ToolTip>
          <Default>selector</Default>
        </Literal>
        <Literal>
          <ID>name</ID>
          <ToolTip>Name of data value</ToolTip>
          <Default>name</Default>
        </Literal>
        <Literal>
          <ID>value</ID>
          <ToolTip>Value to set</ToolTip>
          <Default>value</Default>
        </Literal>
      </Declarations>
      <Code Language="jscript">
        <![CDATA[$$($selector$).data($name$, $value$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

